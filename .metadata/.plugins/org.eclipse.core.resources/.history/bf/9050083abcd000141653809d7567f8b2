package org.usfirst.frc.team5587.robot.commands.modes;

import edu.wpi.first.wpilibj.command.CommandGroup;
import edu.wpi.first.wpilibj.command.Command;

import org.usfirst.frc.team5587.robot.commands.drive.AutoDrive;

/**
 *	Runs the robot straight
 */
public class AutoMode1 extends CommandGroup
{
	private Command autonomous = new AutoDrive( 5000, 0 );
    public  AutoMode1()
    {
    	autonomous.start();
    }
}
